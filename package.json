{
    "publisher": "julienetie",
    "name": "vscode-template-literals",
    "displayName": "Template Literals",
    "description": "Template Literals Syntax Highlighting for HTML and SVG (Without the need of a tag function)",
    "version": "0.6.1",
    "icon": "lambdascript.png",
    "scripts": {
        "publish": "vsce publish"
    },
    "galleryBanner": {
        "color": "#a099ff",
        "theme": "light"
    },
    "repository": {
        "url": "https://github.com/julienetie/vscode-template-literals.git"
    },
    "bugs": {
        "url": "https://github.com/julienetie/vscode-template-literals.git/issues"
    },
    "engines": {
        "vscode": "^1.83.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "template",
        "literal",
        "string",
        "html",
        "svg",
        "highlight",
        "highlighter",
        "highlighting",
        "syntax",
        "javascript"
    ],
    "contributes": {
        "grammars": [
            {
                "injectTo": [
                    "source.html",
                    "source.js",
                    "source.js.jsx",
                    "source.jsx",
                    "source.ts",
                    "source.tsx",
                    "text.html"
                ],
                "scopeName": "inline.template-literals",
                "path": "./syntaxes/template-literals.json",
                "embeddedLanguages": {
                    "meta.embedded.block.html": "html"
                }
            }
        ]
    }
}